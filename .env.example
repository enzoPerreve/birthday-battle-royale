# Environment variables for Birthday Battle Royale frontend

# API Configuration
REACT_APP_API_URL=http://localhost:3000/api

# Firebase Configuration (if using Firebase on frontend)
REACT_APP_FIREBASE_API_KEY=your-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
REACT_APP_FIREBASE_APP_ID=1:123456789:web:abcdef

# App Configuration
REACT_APP_NAME="Birthday Battle Royale"
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_SOUND=false
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_AUTO_REFRESH_INTERVAL=30000

# Development Settings
REACT_APP_DEBUG=false
