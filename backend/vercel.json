{
  "version": 2,
  "routes": [
    {
      "src": "/api/health",
      "dest": "/health.js"
    },
    {
      "src": "/api/users/register",
      "dest": "/register.js"
    },
    {
      "src": "/api/admin/verify",
      "dest": "/verify.js"
    },
    {
      "src": "/(.*)",
      "dest": "/health.js"
    }
  ]
}
